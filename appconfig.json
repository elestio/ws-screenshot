{
  "env": "PROD",
  "version": "1.0.0",
  "title": "WS-Screenshot",
  "description": "Screenshot Service",
  "mainDomain": "screenshot.elest.io",
  "domains": ["*","screenshot.elest.io"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "HSTS": false,
  "rateLimiter" : {
    "requestsPerSecond" : 4
  },
  "CORS": {
      "access-control-allow-origin": "*"
  },
  "apiEndpoints": {
    "/api/screenshot" : {
      "src" : "./API/REST/",
      "handler": "Screenshot.handler"
    }
  },
  "websocketEndpoints": {
    "/ws/screenshot" : {
      "src" : "./API/WS/",
      "upgrade": "Screenshot.upgrade",
      "open": "Screenshot.open",
      "message": "Screenshot.message",
      "close": "Screenshot.close"
    }
  },
  "ApiKey": ""
}